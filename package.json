{
  "name": "docspa",
  "private": true,
  "version": "6.0.0",
  "description": "An Angular-powered documentation SPA",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "lint": "ng lint",
    "lint:fix": "ng lint -- --fix",
    "e2e": "start-server-and-test start http-get://localhost:4200 cypress:run",
    "unit": "jest",
    "test": "run-s lint unit e2e",
    "deploy": "ng deploy --base-href=\"/DocSPA/\"",
    "postversion": "npm run setMetadata && npm run chg:release:lib && git add -A .",
    "chg:release:lib": "cd projects/swimlane/docspa-core && chg release -y",
    "setMetadata": "node ./tools/set-metadata.js",
    "build:core": "ng build @swimlane/docspa-core --prod",
    "postbuild:core": "cpx \"./{README.md,LICENSE}\" ./dist/swimlane/docspa-core && cpx ./projects/swimlane/docspa-core/CHANGELOG.md ./dist/swimlane/docspa-core",
    "build:stackblitz": "ng build @swimlane/docspa-stackblitz --prod",
    "build:libs": "npm run build:core && npm run build:stackblitz",
    "prepublish:core": "npm run build:core",
    "publish:core": "npm publish dist/swimlane/docspa-core",
    "prepublish:stackblitz": "npm run build:stackblitz",
    "publish:stackblitz": "npm publish dist/swimlane/docspa-stackblitz",
    "prepublish:preset": "cd projects/swimlane/docspa-remark-preset && npm run build",
    "publish:preset": "npm publish projects/swimlane/docspa-remark-preset",
    "test:deploy": "cross-env CYPRESS_baseUrl=\"https://swimlane.github.io/DocSPA/\" cypress run --browser chrome",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "unit:watch": "jest --watch",
    "bundle-report": "webpack-bundle-analyzer dist/docspa/stats.json",
    "prepack:core": "npm run build:core",
    "pack:core": "npm pack ./dist/swimlane/docspa-core",
    "prepack:preset": "cd projects/swimlane/docspa-remark-preset && npm run build",
    "pack:preset": "npm pack ./projects/swimlane/docspa-remark-preset/",
    "prepack:stackblitz": "npm run build:stackblitz",
    "pack:stackblitz": "npm pack ./dist/swimlane/docspa-stackblitz",
    "pack": "npm run pack:core && npm run pack:preset && npm run pack:stackblitz"
  },
  "dependencies": {
    "@angular/animations": "^10.2.0",
    "@angular/cdk": "^10.2.5",
    "@angular/common": "^10.2.0",
    "@angular/compiler": "^10.2.0",
    "@angular/core": "^10.2.0",
    "@angular/elements": "^10.2.0",
    "@angular/forms": "^10.2.0",
    "@angular/platform-browser": "^10.2.0",
    "@angular/platform-browser-dynamic": "^10.2.0",
    "@angular/router": "^10.2.0",
    "@angular/service-worker": "^10.2.0",
    "@mapbox/rehype-prism": "^0.5.0",
    "@ngx-loading-bar/core": "^5.1.0",
    "@ngx-loading-bar/http-client": "^5.1.0",
    "@stackblitz/sdk": "^1.5.1",
    "@swimlane/docspa-core": "^6.0.0",
    "@swimlane/docspa-remark-preset": "^6.0.0",
    "@swimlane/docspa-search": "*",
    "@swimlane/ngx-charts": "^16.0.0",
    "@ungap/global-this": "^0.4.0",
    "@webcomponents/custom-elements": "^1.4.2",
    "deepmerge": "^4.2.2",
    "hast-util-to-dom": "^2.0.6",
    "intersection-observer": "^0.11.0",
    "ngx-logger": "^4.1.9",
    "normalize-path": "^3.0.0",
    "path": "^0.12.7",
    "path-browserify": "^1.0.1",
    "path-dirname": "^1.0.2",
    "process": "^0.11.10",
    "request": "^2.88.2",
    "rxjs": "^6.6.3",
    "tslib": "^2.0.3",
    "url": "^0.11.0",
    "url-resolve": "^1.0.0",
    "util": "^0.12.3",
    "vfile": "^4.2.0",
    "vfile-reporter": "^6.0.1",
    "zone.js": "^0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1001.7",
    "@angular-devkit/build-ng-packagr": "^0.1001.7",
    "@angular-devkit/core": "^10.1.7",
    "@angular-devkit/schematics": "^10.1.7",
    "@angular/cli": "^10.1.7",
    "@angular/compiler-cli": "^10.2.0",
    "@angular/language-service": "^10.2.0",
    "@types/jasmine": "^3.5.14",
    "@types/mdast": "^3.0.2",
    "@types/node": "^13.13.15",
    "@types/quick-lru": "^3.0.0",
    "@types/tapable": "^1.0.6",
    "angular-cli-ghpages": "^0.6.2",
    "chai": "^4.1.2",
    "chg": "^0.4.0",
    "codelyzer": "^6.0.1",
    "cpx": "^1.5.0",
    "cross-env": "^7.0.2",
    "cypress": "^5.4.0",
    "jest": "^26.5.3",
    "jest-fetch-mock": "^3.0.3",
    "jest-preset-angular": "^8.3.1",
    "ng-packagr": "^10.1.2",
    "npm-run-all": "^4.1.5",
    "prismjs": "^1.22.0",
    "start-server-and-test": "^1.11.5",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "webpack-bundle-analyzer": "^3.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/docspa.git"
  },
  "keywords": [
    "angular",
    "documentation"
  ],
  "authors": [
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/docspa/issues"
  },
  "homepage": "https://swimlane.github.io/DocSPA/",
  "browser": {
    "crypto": false
  }
}
