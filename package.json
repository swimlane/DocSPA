{
  "name": "docspa",
  "private": true,
  "version": "4.0.0",
  "description": "An Angular-powered documentation SPA",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postversion": "npm run setMetadata && npm run chg:release:lib && git add -A .",
    "chg:release:lib": "cd projects/swimlane/docspa-core && chg release -y",
    "setMetadata": "ts-node ./tools/set-metadata.ts",
    "build:core": "ng build @swimlane/docspa-core --prod",
    "build:stackblitz": "ng build @swimlane/docspa-stackblitz",
    "build:libs": "npm run build:core && npm run build:stackblitz",
    "prebuild:release": "rimraf dist",
    "build:release": "cross-env NODE_ENV=production ng build --prod --stats-json --no-aot --no-build-optimizer --base-href=\"/DocSPA/\"",
    "prepublish:core": "npm run build:core",
    "publish:core": "cd dist/swimlane/docspa-core && npm publish",
    "prepublish:stackblitz": "npm run build:stackblitz",
    "publish:stackblitz": "cd dist/swimlane/docspa-stackblitz && npm publish",
    "publish:libs": "npm run publish:core && npm run publish:stackblitz",
    "test": "jest",
    "ci": "jest",
    "test:deploy": "cross-env CYPRESS_baseUrl=\"https://swimlane.github.io/DocSPA/\" cypress run --browser chrome",
    "test:docs": "start-server-and-test start http-get://localhost:4200 cypress:run",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "lint": "ng lint",
    "e2e": "npm run test:docs",
    "predeploy": "npm run build:release",
    "deploy": "angular-cli-ghpages --dir ./dist/docspa",
    "test:watch": "jest --watch",
    "bundle-report": "webpack-bundle-analyzer dist/docspa/stats.json",
    "pack:core": "npm run build:core && npm pack ./dist/swimlane/docspa-core",
    "pack:stackblitz": "npm run build:stackblitz && npm pack ./dist/swimlane/docspa-stackblitz",
    "pack": "npm run pack:core && npm run pack:stackblitz"
  },
  "dependencies": {
    "@angular/animations": "^8.0.0",
    "@angular/common": "^8.0.0",
    "@angular/compiler": "^8.0.0",
    "@angular/core": "^8.0.0",
    "@angular/elements": "^8.0.0",
    "@angular/forms": "^8.0.0",
    "@angular/http": "^7.2.10",
    "@angular/platform-browser": "^8.0.0",
    "@angular/platform-browser-dynamic": "^8.0.0",
    "@angular/pwa": "^0.13.6",
    "@angular/router": "^8.0.0",
    "@angular/service-worker": "^8.0.0",
    "@mapbox/rehype-prism": "^0.3.1",
    "@ngx-loading-bar/http-client": "^2.1.0",
    "@rigor789/remark-autolink-headings": "^5.1.0",
    "@stackblitz/sdk": "^1.2.0",
    "@swimlane/docspa-remark-preset": "^1.3.0",
    "@swimlane/ngx-charts": "^11.1.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "core-js": "^2.6.5",
    "deepmerge": "^3.2.0",
    "hast-util-to-dom": "^1.1.0",
    "md-attr-parser": "^1.2.1",
    "mdast-util-toc": "^3.1.0",
    "mermaid": "^8.0.0-rc.8",
    "ngx-logger": "^3.3.12",
    "parse-numeric-range": "0.0.2",
    "path": "^0.12.7",
    "path-browserify": "^1.0.0",
    "process": "^0.11.10",
    "quick-lru": "^3.0.0",
    "rehype-autolink-headings": "^2.0.2",
    "rehype-dom-stringify": "^1.0.4",
    "rehype-raw": "^4.0.0",
    "rehype-stringify": "^5.0.0",
    "remark": "^10.0.1",
    "remark-attr": "^0.8.3",
    "remark-collapse": "^0.1.2",
    "remark-custom-blockquotes": "^1.0.0",
    "remark-custom-blocks": "^2.3.2",
    "remark-frontmatter": "^1.2.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-html-emoji-image": "^1.0.0",
    "remark-html-katex": "^1.1.1",
    "remark-lint-no-dead-urls": "^0.4.1",
    "remark-math": "^1.0.6",
    "remark-mermaid": "^0.2.0",
    "remark-parse": "^6.0.3",
    "remark-parse-yaml": "0.0.2",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "remark-rehype": "^4.0.0",
    "remark-shortcodes": "^0.2.1",
    "remark-slug": "^5.1.0",
    "remark-toc": "^5.0.0",
    "remark-validate-links": "^8.0.2",
    "rxjs": "^6.5.2",
    "tapable": "^1.1.0",
    "unified": "^7.0.0",
    "url": "^0.11.0",
    "url-resolve": "^1.0.0",
    "util": "^0.11.0",
    "vfile": "^4.0.0",
    "vfile-reporter": "^5.0.0",
    "zone.js": "^0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "^7.3.6",
    "@angular/compiler-cli": "^8.0.0",
    "@angular/language-service": "^8.0.0",
    "@types/jasmine": "^3.3.12",
    "@types/mdast": "^3.0.2",
    "@types/node": "^11.11.6",
    "@types/quick-lru": "^3.0.0",
    "@types/unist": "^2.0.3",
    "@types/vfile": "^3.0.2",
    "angular-cli-ghpages": "^0.5.2",
    "chai": "^4.1.2",
    "codelyzer": "^4.5.0",
    "cross-env": "^5.2.0",
    "cypress": "^3.2.0",
    "jest": "^24.5.0",
    "jest-fetch-mock": "^2.1.1",
    "jest-preset-angular": "^7.0.1",
    "ng-packagr": "^5.2.0",
    "prismjs": "^1.15.0",
    "rimraf": "^2.6.3",
    "start-server-and-test": "^1.7.12",
    "trash": "^5.0.0",
    "ts-node": "^8.0.3",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.0",
    "tslint": "^5.14.0",
    "typescript": "^3.4.5",
    "webpack-bundle-analyzer": "^3.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/docspa.git"
  },
  "keywords": [
    "angular",
    "documentation"
  ],
  "authors": [
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/docspa/issues"
  },
  "homepage": "https://swimlane.github.io/DocSPA/"
}
