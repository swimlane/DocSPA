{
  "name": "docspa",
  "private": true,
  "version": "1.5.2",
  "description": "An Angular-powered documentation SPA",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "postversion": "npm run setMetadata",
    "setMetadata": "ts-node ./tools/set-metadata.ts",
    "prebuild:release": "rimraf dist",
    "build:lib": "ng build @swimlane/docspa-core ---prod",
    "build:release": "cross-env NODE_ENV=production ng build --prod --no-aot --no-build-optimizer --base-href=\"/DocSPA/\"",
    "prepublish:lib": "npm run build:lib",
    "publish:lib": "cd dist/swimlane/docspa-core && npm publish",
    "test": "jest",
    "test:deploy": "cross-env CYPRESS_baseUrl=\"https://swimlane.github.io/DocSPA/\" cypress run --browser chrome",
    "test:docs": "start-server-and-test start http-get://localhost:4200 cypress:run",
    "test:preset": "mocha ./projects/swimlane/docspa-remark-preset/tests/**/*.ts -r ts-node/register",
    "test:preset:watch": "mocha ./projects/swimlane/docspa-remark-preset/tests/**/*.ts --compilers ts:ts-node/register --watch",
    "test:core": "ng test @swimlane/docspa-core",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run --browser chrome",
    "lint": "ng lint",
    "e2e": "npm run test:docs",
    "predeploy": "npm run build:release",
    "deploy": "angular-cli-ghpages --dir ./dist/docspa",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "@angular/animations": "^6.1.1",
    "@angular/common": "^6.1.1",
    "@angular/compiler": "^6.1.1",
    "@angular/core": "^6.1.1",
    "@angular/elements": "^6.1.1",
    "@angular/forms": "^6.1.1",
    "@angular/http": "^6.1.1",
    "@angular/platform-browser": "^6.1.1",
    "@angular/platform-browser-dynamic": "^6.1.1",
    "@angular/pwa": "^0.7.5",
    "@angular/router": "^6.1.1",
    "@angular/service-worker": "^6.1.1",
    "@mapbox/rehype-prism": "^0.2.0",
    "@ngx-loading-bar/http-client": "^2.1.0",
    "@paperist/types-remark": "^0.1.3",
    "@rigor789/remark-autolink-headings": "^5.1.0",
    "@stackblitz/sdk": "^1.2.0",
    "@swimlane/docspa-remark-preset": "^1.0.0",
    "@swimlane/ngx-charts": "^8.1.0",
    "@webcomponents/custom-elements": "^1.1.2",
    "core-js": "^2.5.4",
    "deepmerge": "^2.1.1",
    "md-attr-parser": "^1.1.5",
    "mdast-util-toc": "^2.0.1",
    "mermaid": "^8.0.0-rc.8",
    "ngx-logger": "^3.0.5",
    "parse-numeric-range": "0.0.2",
    "path": "^0.12.7",
    "path-browserify": "^1.0.0",
    "process": "^0.11.10",
    "quick-lru": "^1.1.0",
    "rehype-autolink-headings": "^2.0.2",
    "rehype-stringify": "^3.0.0",
    "remark": "^9.0.0",
    "remark-attr": "^0.6.2",
    "remark-collapse": "^0.1.2",
    "remark-custom-blockquotes": "^1.0.0",
    "remark-custom-blocks": "^2.2.0",
    "remark-frontmatter": "^1.2.0",
    "remark-gemoji-to-emoji": "^1.1.0",
    "remark-html": "^7.0.0",
    "remark-html-emoji-image": "^1.0.0",
    "remark-html-katex": "^1.1.1",
    "remark-lint-no-dead-urls": "^0.4.1",
    "remark-math": "^1.0.4",
    "remark-mermaid": "^0.2.0",
    "remark-parse": "^5.0.0",
    "remark-parse-yaml": "0.0.1",
    "remark-preset-lint-consistent": "^2.0.2",
    "remark-preset-lint-markdown-style-guide": "^2.1.2",
    "remark-preset-lint-recommended": "^3.0.2",
    "remark-shortcodes": "^0.2.1",
    "remark-slug": "^5.1.0",
    "remark-toc": "^5.0.0",
    "remark-validate-links": "^7.1.0",
    "rxjs": "^6.2.2",
    "unified": "^7.0.0",
    "url": "^0.11.0",
    "url-resolve": "^1.0.0",
    "vfile": "^3.0.0",
    "vfile-reporter": "^5.0.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.6.8",
    "@angular-devkit/build-ng-packagr": "~0.6.8",
    "@angular/cli": "^6.2.0-beta.1",
    "@angular/compiler-cli": "^6.1.1",
    "@angular/language-service": "^6.1.1",
    "@types/jasmine": "^2.8.8",
    "@types/node": "^8.10.20",
    "@types/quick-lru": "^1.1.0",
    "angular-cli-ghpages": "^0.5.2",
    "chai": "^4.1.2",
    "codelyzer": "~4.2.1",
    "cross-env": "^5.2.0",
    "cypress": "^3.1.0",
    "jest": "23.5.0",
    "jest-preset-angular": "6.0.0",
    "ng-packagr": "^3.0.0-rc.2",
    "prismjs": "^1.15.0",
    "rimraf": "^2.6.2",
    "start-server-and-test": "^1.7.0",
    "ts-node": "~5.0.1",
    "tslib": "^1.7.1",
    "tslint": "~5.9.1",
    "typescript": "~2.9.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/swimlane/docspa.git"
  },
  "keywords": [
    "angular",
    "documentation"
  ],
  "authors": [
    "Jayson Harshbarger"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/swimlane/docspa/issues"
  },
  "homepage": "https://swimlane.github.io/DocSPA/"
}
